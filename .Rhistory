#'
#' @param vcffilepath
#'
#' @return
#' @export
#'
#' @examples
readVCF = function(vcffilepath){
vavcf = VariantAnnotation::readVcfAsVRanges(vcffilepath)
vavcf = dplyr::filter(tibble::as_tibble(vavcf), AF !=0 | alt == "<CNV>")
vavcf$rowid = 1:nrow(vavcf)
return(vavcf)
}
